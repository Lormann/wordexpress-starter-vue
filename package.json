{
  "name": "wordexpress-starter-kit-vue",
  "version": "2.1.0",
  "description": "WordPress with Vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramsaylanier/VuePress"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
